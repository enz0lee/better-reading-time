{
  "name": "wxt-react-starter",
  "description": "Estimate the total reading time and track remaining reading time dynamically based on your scroll position.",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:edge": "wxt -b edge",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "prepare": "wxt prepare",
    "postinstall": "wxt prepare",
    "format": "prettier --write .",
    "lint": "eslint entrypoints --ext .ts,.tsx",
    "lint:fix": "eslint entrypoints --ext .ts,.tsx --fix ."
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/module-react": "^1.1.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "wxt": "^0.20.6"
  }
}
